<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "http://www.asahi-net.or.jp/~cs8k-cyu/bulletml/bulletml.dtd">
<bulletml>
  <action label="top">
    <repeat>
      <times>5</times>
      <action>
        <repeat>
          <times>5</times>
          <action>
            <fire>
              <bullet />
            </fire>
          </action>
        </repeat>
      </action>
    </repeat>
  </action>
</bulletml>
